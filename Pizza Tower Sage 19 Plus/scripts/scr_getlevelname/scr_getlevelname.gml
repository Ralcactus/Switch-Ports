function scr_getlevelname(argument0)
{
    switch (argument0)
    {
        case "tutorial":
            return "Buried Pizza Ruins";
            break;
        
        case "entry":
            return "Archaic Entry";
            break;
        
        case "medieval":
            return "Pizzascape";
            break;
        
        case "ruin":
            return "Ancient Cheese";
            break;
        
        case "dungeon":
            return "Bloodsauce Dungeon";
            break;
        
        case "dragonslair":
            return "Dragons Lair";
            break;
    }
}
